<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Calendar - Home</title>

    <link href="/netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <div class="header-top">
        <form action="/users/logout" method="GET">
            <button class="doidera btn btn-default">
                <span class="glyphicon glyphicon-ok">Logout</span>
            </button>
        </form>
        <h1 class="title-page">CALENDAR</h1>
    </div>

    <form action="/calendars" method="POST">
        <label class="form-span" for="input1">Name</label>
        <input type="text" name="name" placeholder="event name" id="input1">

        <label class="form-span" for="select1">Day</label>
        <select class="form-control" name="day" id="select1">
            <option value="28">28</option>
            <option value="29">29</option>
            <option value="30">30</option>
            <option value="31">31</option>
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
        </select>

        <label class="form-span" for="select2">Hour</label>
        <select class="form-control" name="hour" id="select2">
            <option value="1:00">1:00</option>
            <option value="2:00">2:00</option>
            <option value="3:00">3:00</option>
            <option value="4:00">4:00</option>
            <option value="5:00">5:00</option>
        </select>

        <button type="submit" class="btn btn-default full-width">
            <span class="glyphicon glyphicon-ok">Create Event</span>
        </button>
    </form>

    <table>
        <thead>
            <tr>
                <th></th>
                <th>
                    <span class="day">28</span>
                    <span class="long">Monday</span>
                    <span class="short">Mon</span>
                </th>
                <th>
                    <span class="day active">29</span>
                    <span class="long">Tuesday</span>
                    <span class="short">Tue</span>
                </th>
                <th>
                    <span class="day">30</span>
                    <span class="long">Wendsday</span>
                    <span class="short">We</span>
                </th>
                <th>
                    <span class="day">31</span>
                    <span class="long">Thursday</span>
                    <span class="short">Thur</span>
                </th>
                <th>
                    <span class="day">1</span>
                    <span class="long">Friday</span>
                    <span class="short">Fri</span>
                </th>
                <th>
                    <span class="day">2</span>
                    <span class="long">Saturday</span>
                    <span class="short">Sat</span>
                </th>
                <th>
                    <span class="day">3</span>
                    <span class="long">Sunday</span>
                    <span class="short">Sun</span>
                </th>
            </tr>
        </thead>

        {{#each v291}}
        <tbody>
            <tr>
                <td class="hour"><span>1:00</span></td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
            </tr>

            <tr>
                <td class="hour"><span>2:00</span></td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
            </tr>

            <tr>
                <td class="hour"><span>3:00</span></td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
            </tr>

            <tr>
                <td class="hour"><span>4:00</span></td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
            </tr>

            <tr>
                <td class="hour"><span>5:00</span></td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
                <td>{{this.name}}</td>
            </tr>

        </tbody>
        {{/each}}
    </table>
</body>

</html>